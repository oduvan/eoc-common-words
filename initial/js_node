
export function commonWords(first: string, second: string): string {
    return "";
}

import * as assert from 'assert';

console.log('Example')
console.log(commonWords("hello,world", "hello,earth"))

assert.equal(commonWords("hello,world", "hello,earth"), "hello");
assert.equal(commonWords("one,two,three", "four,five,six"), "");
assert.equal(commonWords("one,two,three", "four,five,one,two,six,three"), "one,three,two");
